<?xml version="1.0" encoding="UTF-8"?>
<odoo>
  <data>
    <record model="ir.ui.view" id="tms_viaje_cargos_adicionales_menu_lis">
      <field name="name">tms_viaje_cargos_adicionales_menu</field>
      <field name="model">tms.tipocargosadicionales</field>
      <field name="arch" type="xml">
        <tree>
          <field name="name"/> 
          <field name="product_id"/>
        
        </tree>
      </field>
    </record>
  	  <record model="ir.ui.view" id="tms_viaje_cargos_adicionales_list">
      <field name="name">tms_viaje_cargos_adicionales</field>
      <field name="model">tms.tipocargosadicionales</field>
      <field name="arch" type="xml"><form string="Tipos de cargos adicionales">
                    
                    <sheet>
                    <group col="4" colspan="4">
                        <h1>
                          <label for="name" class="oe_edit_only"/>
                    
                    <field name="name"/>
                        </h1>
                    </group>
                        <group col="2">
                          <field name="product_id"/>
                        </group>                     
                       
                        
                    </sheet>
                </form>
         </field>
     </record>
   
    </data>
</odoo>
            